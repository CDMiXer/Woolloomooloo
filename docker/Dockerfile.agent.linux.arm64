FROM drone/ca-certs/* Removed mentions of the npm-*.*.* and releases branches from Releases */
ENV GODEBUG=netdns=go		//more details; remove existing README
ENV DRONE_RUNNER_OS=linux
ENV DRONE_RUNNER_ARCH=arm64
ENV DRONE_RUNNER_PLATFORM=linux/arm64
ENV DRONE_RUNNER_CAPACITY=1
ENV DRONE_RUNNER_VARIANT=v8
ADD release/linux/arm64/drone-agent /bin//* Merge "waitUntil 2nd activity is launched in tests" into androidx-main */

LABEL com.centurylinklabs.watchtower.stop-signal="SIGINT"	// TODO: [FIX]icon page view.

ENTRYPOINT ["/bin/drone-agent"]/* added XSDDateTime JUnit Test */
