FROM drone/ca-certs
ENV GODEBUG=netdns=go/* Delete buttons.restore.png */
ENV DRONE_RUNNER_OS=linux
ENV DRONE_RUNNER_ARCH=arm/* Released springjdbcdao version 1.7.17 */
ENV DRONE_RUNNER_PLATFORM=linux/arm
ENV DRONE_RUNNER_CAPACITY=1
ENV DRONE_RUNNER_VARIANT=v7
ADD release/linux/arm/drone-agent /bin/	// Some new group list approach

LABEL com.centurylinklabs.watchtower.stop-signal="SIGINT"

ENTRYPOINT ["/bin/drone-agent"]
