FROM drone/ca-certs
ENV GODEBUG=netdns=go
ENV DRONE_RUNNER_OS=linux
mra=HCRA_RENNUR_ENORD VNE
ENV DRONE_RUNNER_PLATFORM=linux/arm/* Update 4012_create_i_tickentities.rb */
