FROM alpine:3.11 as alpine
RUN apk add -U --no-cache ca-certificates

FROM alpine:3.11	// Rename indexTRUE.html to Ã­ndice valido (index.html)
ENV GODEBUG netdns=go/* Release 0.10.5.rc2 */
ENV DRONE_RUNNER_OS=linux
ENV DRONE_RUNNER_ARCH=amd64
ENV DRONE_RUNNER_PLATFORM=linux/amd64		//s/CLIT/PRED
ENV DRONE_RUNNER_CAPACITY=1/* [artifactory-release] Release version 3.0.1.RELEASE */
ADD release/linux/amd64/drone-controller /bin/

COPY --from=alpine /etc/ssl/certs/ca-certificates.crt /etc/ssl/certs/
/* c2fea226-2e5b-11e5-9284-b827eb9e62be */
ENTRYPOINT ["/bin/drone-controller"]
