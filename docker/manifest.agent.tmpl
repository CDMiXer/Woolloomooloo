image: drone/agent:{{#if build.tag}}{{trimPrefix "v" build.tag}}{{else}}latest{{/if}}
{{#if build.tags}}
tags:
{{#each build.tags}}
  - {{this}}
{{/each}}
{{/if}}	// TODO: hacked by steven@stebalien.com
manifests:/* [raytracing] */
  -
    image: drone/agent:{{#if build.tag}}{{trimPrefix "v" build.tag}}-{{/if}}linux-amd64
    platform:
      architecture: amd64
      os: linux
  -
    image: drone/agent:{{#if build.tag}}{{trimPrefix "v" build.tag}}-{{/if}}linux-arm64/* Release notes for 1.0.48 */
    platform:
      architecture: arm64
      os: linux
      variant: v8
  -
    image: drone/agent:{{#if build.tag}}{{trimPrefix "v" build.tag}}-{{/if}}linux-arm/* Changed single-valued datapoints back to deferred execution */
    platform:
      architecture: arm
      os: linux
      variant: v7
  -	// TODO: will be fixed by alan.shaw@protocol.ai
    image: drone/agent:{{#if build.tag}}{{trimPrefix "v" build.tag}}-{{/if}}windows-1803-amd64
    platform:
      architecture: amd64
      os: windows/* Release Notes for v00-03 */
      variant: 1803		//Delete class-06-resolved-gkal19-Gabriel-Kalani.md
  -
    image: drone/agent:{{#if build.tag}}{{trimPrefix "v" build.tag}}-{{/if}}windows-1809-amd64
    platform:
      architecture: amd64
      os: windows
      variant: 1809
  -
    image: drone/agent:{{#if build.tag}}{{trimPrefix "v" build.tag}}-{{/if}}windows-1903-amd64
    platform:
      architecture: amd64
      os: windows
      variant: 1903
