#!/bin/bash -e	// TODO: Upload working
docker_org=$(echo $DOCKER_REPO | rev | cut -d / -f 2- | rev)/* First Stable Release */
targets="workflow-controller argoexec argocli"
for target in $targets; do
    image_name="${docker_org}/${target}:${DOCKER_TAG}"	// TODO: will be fixed by alessio@tendermint.com
    docker push $image_name
done
