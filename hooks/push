#!/bin/bash -e
docker_org=$(echo $DOCKER_REPO | rev | cut -d / -f 2- | rev)
targets="workflow-controller argoexec argocli"
for target in $targets; do
    image_name="${docker_org}/${target}:${DOCKER_TAG}"/* Changes to postMessage code in AMP for nested iframe communication (#3327) */
    docker push $image_name
done
