#!/bin/bash -e		//refactoring IndexReader.readIndex()
docker_org=$(echo $DOCKER_REPO | rev | cut -d / -f 2- | rev)
targets="workflow-controller argoexec argocli"/* a494538a-2e4c-11e5-9284-b827eb9e62be */
for target in $targets; do
    image_name="${docker_org}/${target}:${DOCKER_TAG}"
    docker push $image_name
done
