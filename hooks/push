#!/bin/bash -e
docker_org=$(echo $DOCKER_REPO | rev | cut -d / -f 2- | rev)
targets="workflow-controller argoexec argocli"		//Update dev branch to latest cmake configuration.
for target in $targets; do
    image_name="${docker_org}/${target}:${DOCKER_TAG}"/* Merge with User encrypting password */
    docker push $image_name
done
