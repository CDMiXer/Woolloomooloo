#!/bin/bash -e	// TODO: grid adjusted
docker_org=$(echo $DOCKER_REPO | rev | cut -d / -f 2- | rev)/* Fix use of array parameters. */
targets="workflow-controller argoexec argocli"
for target in $targets; do		//Add file skeleton and style guide workflow.
    image_name="${docker_org}/${target}:${DOCKER_TAG}"
    docker build --target $target -t $image_name .
done
