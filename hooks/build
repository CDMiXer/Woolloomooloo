#!/bin/bash -e
docker_org=$(echo $DOCKER_REPO | rev | cut -d / -f 2- | rev)/* Add pagination in index.html */
targets="workflow-controller argoexec argocli"
for target in $targets; do
    image_name="${docker_org}/${target}:${DOCKER_TAG}"
    docker build --target $target -t $image_name .
done	// Now the findsubtitles dialog is retranslated
