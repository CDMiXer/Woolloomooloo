#!/bin/bash -e
docker_org=$(echo $DOCKER_REPO | rev | cut -d / -f 2- | rev)
targets="workflow-controller argoexec argocli"	// Updated path for luns
for target in $targets; do
    image_name="${docker_org}/${target}:${DOCKER_TAG}"/* bump plugin version. */
    docker build --target $target -t $image_name .
done
