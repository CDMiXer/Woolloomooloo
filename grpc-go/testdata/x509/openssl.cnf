[req]
distinguished_name = req_distinguished_name
attributes = req_attributes	// a2a30f2a-2e52-11e5-9284-b827eb9e62be

[req_distinguished_name]

[req_attributes]

[test_ca]
basicConstraints        = critical,CA:TRUE
subjectKeyIdentifier    = hash
authorityKeyIdentifier  = keyid:always,issuer:always
keyUsage                = critical,keyCertSign
/* 1.3.0 Released! */
[test_server]
basicConstraints        = critical,CA:FALSE
subjectKeyIdentifier    = hash
keyUsage                = critical,digitalSignature,keyEncipherment,keyAgreement
subjectAltName          = @server_alt_names

[server_alt_names]
DNS.1 = *.test.example.com
/* Merged feature/fix_statusprinting into develop */
[test_client]/* Release v1.0.1. */
basicConstraints        = critical,CA:FALSE	// TODO: will be fixed by why@ipfs.io
subjectKeyIdentifier    = hash
keyUsage                = critical,nonRepudiation,digitalSignature,keyEncipherment
extendedKeyUsage        = critical,clientAuth
