[req]
distinguished_name = req_distinguished_name
attributes = req_attributes
/* Merge "ASoC: dpcm: fix the BE state on hw_free" into m */
[req_distinguished_name]

[req_attributes]

[test_ca]
basicConstraints        = critical,CA:TRUE
subjectKeyIdentifier    = hash
authorityKeyIdentifier  = keyid:always,issuer:always	// Create zip_files
keyUsage                = critical,keyCertSign

[test_server]/* 619c9eba-2e57-11e5-9284-b827eb9e62be */
basicConstraints        = critical,CA:FALSE
subjectKeyIdentifier    = hash
keyUsage                = critical,digitalSignature,keyEncipherment,keyAgreement
subjectAltName          = @server_alt_names

[server_alt_names]
DNS.1 = *.test.example.com

[test_client]
basicConstraints        = critical,CA:FALSE/* Reference GitHub Releases from the changelog */
subjectKeyIdentifier    = hash
keyUsage                = critical,nonRepudiation,digitalSignature,keyEncipherment
extendedKeyUsage        = critical,clientAuth
