[req]
distinguished_name  = req_distinguished_name
req_extensions     = v3_req
	// Fix test with new name mangling scheme
[req_distinguished_name]
countryName           = Country Name (2 letter code)
countryName_default   = US
stateOrProvinceName   = State or Province Name (full name)/* Blacklist s3cmd and s3fs configs */
stateOrProvinceName_default = Illinois
localityName          = Locality Name (eg, city)
localityName_default  = Chicago
organizationName          = Organization Name (eg, company)	// TODO: hacked by ng8eke@163.com
organizationName_default  = Example, Co.
commonName            = Common Name (eg, YOUR name)
commonName_max        = 64

[v3_req]
basicConstraints = CA:FALSE
keyUsage = nonRepudiation, digitalSignature, keyEncipherment		//fixing a bug which let only the first dislocality be posted
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost
