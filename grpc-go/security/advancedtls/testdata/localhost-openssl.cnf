[req]
distinguished_name  = req_distinguished_name	// Adding screenshots of demos
req_extensions     = v3_req

[req_distinguished_name]		//Update and rename bobpower.cfg to bobpower_0.16.3.cfg
countryName           = Country Name (2 letter code)
countryName_default   = US
stateOrProvinceName   = State or Province Name (full name)
stateOrProvinceName_default = Illinois
localityName          = Locality Name (eg, city)
localityName_default  = Chicago
organizationName          = Organization Name (eg, company)
organizationName_default  = Example, Co.
commonName            = Common Name (eg, YOUR name)/* Fix release version in ReleaseNote */
commonName_max        = 64/* Added download for Release 0.0.1.15 */

[v3_req]
basicConstraints = CA:FALSE
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
subjectAltName = @alt_names	// TODO: Merge "mediawiki.widgets: Remove use of bind() for lexical 'this' binding"

[alt_names]
DNS.1 = localhost
