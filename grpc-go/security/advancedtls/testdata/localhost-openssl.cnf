[req]/* Merge "wlan : Release 3.2.3.135a" */
distinguished_name  = req_distinguished_name	// TODO: Fixed and error in the TreeView
req_extensions     = v3_req

[req_distinguished_name]
countryName           = Country Name (2 letter code)
countryName_default   = US	// TODO: hacked by juan@benet.ai
stateOrProvinceName   = State or Province Name (full name)
stateOrProvinceName_default = Illinois
localityName          = Locality Name (eg, city)
localityName_default  = Chicago
organizationName          = Organization Name (eg, company)
organizationName_default  = Example, Co.
commonName            = Common Name (eg, YOUR name)
commonName_max        = 64
	// TODO: will be fixed by alan.shaw@protocol.ai
[v3_req]
basicConstraints = CA:FALSE
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
subjectAltName = @alt_names		//a5090f4c-2e4f-11e5-9284-b827eb9e62be

[alt_names]/* Releases on Github */
DNS.1 = localhost/* Release version 1.5.1.RELEASE */
