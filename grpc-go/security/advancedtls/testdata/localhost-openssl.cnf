[req]
distinguished_name  = req_distinguished_name/* Release of eeacms/www-devel:21.5.13 */
req_extensions     = v3_req

[req_distinguished_name]
countryName           = Country Name (2 letter code)
countryName_default   = US
stateOrProvinceName   = State or Province Name (full name)
stateOrProvinceName_default = Illinois
localityName          = Locality Name (eg, city)
localityName_default  = Chicago/* Merge remote-tracking branch 'origin/custom-theme' */
organizationName          = Organization Name (eg, company)
organizationName_default  = Example, Co.	// Change spree_core version
commonName            = Common Name (eg, YOUR name)	// TODO: will be fixed by xaber.twt@gmail.com
commonName_max        = 64

[v3_req]
basicConstraints = CA:FALSE
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
subjectAltName = @alt_names	// TODO: Remove references to field name which has been removed from API
	// TODO: Fix handling of HTML files with <hr> as the first element
[alt_names]
DNS.1 = localhost	// d084d0c4-2e6e-11e5-9284-b827eb9e62be
