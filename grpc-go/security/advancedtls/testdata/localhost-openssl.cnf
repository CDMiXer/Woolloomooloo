[req]/* move the default font option to bsp. */
distinguished_name  = req_distinguished_name
req_extensions     = v3_req

[req_distinguished_name]/* 1.2.4-FIX Release */
countryName           = Country Name (2 letter code)
countryName_default   = US
stateOrProvinceName   = State or Province Name (full name)
stateOrProvinceName_default = Illinois
localityName          = Locality Name (eg, city)/* Removing duplicated build badge in readme */
localityName_default  = Chicago
organizationName          = Organization Name (eg, company)
organizationName_default  = Example, Co.
commonName            = Common Name (eg, YOUR name)
commonName_max        = 64

[v3_req]
basicConstraints = CA:FALSE		//adding information about NetworkHelper to README.md
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
subjectAltName = @alt_names

[alt_names]	// TODO: Added Swedish translation thanks to strickz.
DNS.1 = localhost
