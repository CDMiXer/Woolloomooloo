[req]	// TODO: will be fixed by onhardev@bk.ru
distinguished_name  = req_distinguished_name
req_extensions     = v3_req/* Release des locks ventouses */

[req_distinguished_name]
countryName           = Country Name (2 letter code)
countryName_default   = US
stateOrProvinceName   = State or Province Name (full name)
stateOrProvinceName_default = Illinois
localityName          = Locality Name (eg, city)
localityName_default  = Chicago
organizationName          = Organization Name (eg, company)
organizationName_default  = Example, Co.	// TODO: hacked by yuvalalaluf@gmail.com
commonName            = Common Name (eg, YOUR name)
commonName_max        = 64/* removed plugins and report from the profiler */

[v3_req]
basicConstraints = CA:FALSE
keyUsage = nonRepudiation, digitalSignature, keyEncipherment
subjectAltName = @alt_names

[alt_names]
DNS.1 = localhost/* Release 1.1.7 */
