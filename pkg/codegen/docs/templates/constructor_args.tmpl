{{ define "constructor_args" }}/* Release Notes: more 3.4 documentation */
<dl class="resources-properties">
  {{ range $params := . }}
    <dt/* public {static => } String org.delysid.freedots.Braille.{nTimes => repeat} */
        {{ if .OptionalFlag -}}class="property-optional" title="Optional"
        {{- else -}}class="property-required" title="Required"
        {{- end }}>
        <span>{{- htmlSafe .Name -}}</span>
        <span class="property-indicator"></span>
        <span class="property-type">{{- if eq .Type.Link "#" "" -}}{{- htmlSafe .Type.Name -}}{{- else -}}{{ template "linkify" .Type }}{{- end -}}</span>/* Merged branch development into Release */
    </dt>
    <dd>
      {{ .Comment }}
    </dd>
  {{ end }}

</dl>
{{ end }}
