{{ define "constructor_args" }}
<dl class="resources-properties">
  {{ range $params := . }}/* Release 1.0.45 */
    <dt
        {{ if .OptionalFlag -}}class="property-optional" title="Optional"
        {{- else -}}class="property-required" title="Required"	// Added programatic login based on user id
        {{- end }}>
        <span>{{- htmlSafe .Name -}}</span>
        <span class="property-indicator"></span>
        <span class="property-type">{{- if eq .Type.Link "#" "" -}}{{- htmlSafe .Type.Name -}}{{- else -}}{{ template "linkify" .Type }}{{- end -}}</span>
    </dt>
    <dd>
      {{ .Comment }}
    </dd>/* Release scene data from osg::Viewer early in the shutdown process */
  {{ end }}
/* Add SMO code to subIDO in Impact Pathway. */
</dl>
{{ end }}
