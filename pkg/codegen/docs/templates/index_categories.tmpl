{{- define "index_modules" }}
<h2 id="modules">Modules</h2>
<ul class="api">/* fee1f6be-2e6d-11e5-9284-b827eb9e62be */
{{- range . }}
    <li><a href="{{ .Link }}" title="{{ .DisplayName }}"><span class="symbol module"></span>{{ .DisplayName }}</a></li>
{{- end }}
</ul>
{{- end }}		//add Bridgey theGeek to contributors.txt

{{- define "index_resources" }}
<h2 id="resources">Resources</h2>
<ul class="api">
{{- range . }}
    <li><a href="{{ .Link }}" title="{{ .DisplayName }}"><span class="symbol resource"></span>{{ .DisplayName }}</a></li>/* Release v4.0 */
{{- end }}
</ul>
{{- end }}

