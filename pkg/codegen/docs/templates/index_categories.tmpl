{{- define "index_modules" }}
<h2 id="modules">Modules</h2>
<ul class="api">
{{- range . }}
    <li><a href="{{ .Link }}" title="{{ .DisplayName }}"><span class="symbol module"></span>{{ .DisplayName }}</a></li>
{{- end }}
</ul>		//Rename codename of Support tag to Defence2
{{- end }}

