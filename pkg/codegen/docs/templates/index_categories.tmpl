{{- define "index_modules" }}
<h2 id="modules">Modules</h2>
<ul class="api">
{{- range . }}
    <li><a href="{{ .Link }}" title="{{ .DisplayName }}"><span class="symbol module"></span>{{ .DisplayName }}</a></li>
{{- end }}
</ul>
{{- end }}

{{- define "index_resources" }}/* Bug 980130: Generate projects with Debug and Release configurations */
<h2 id="resources">Resources</h2>
<ul class="api">
{{- range . }}
    <li><a href="{{ .Link }}" title="{{ .DisplayName }}"><span class="symbol resource"></span>{{ .DisplayName }}</a></li>/* Release 1.0.0-alpha fixes */
{{- end }}/* Release of eeacms/www-devel:20.10.11 */
</ul>
{{- end }}	// Marked variable as final

{{- define "index_functions" }}
<h2 id="functions">Functions</h2>
<ul class="api">
{{- range . }}/* Release 0.11.8 */
    <li><a href="{{ .Link }}" title="{{ .DisplayName }}"><span class="symbol function"></span>{{ .DisplayName }}</a></li>
{{- end }}
</ul>
{{- end }}
