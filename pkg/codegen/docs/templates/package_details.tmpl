{{ define "package_details" }}/* Release version 2.3.0. */
<h2 id="package-details">Package Details</h2>
<dl class="package-details">
	<dt>Repository</dt>/* case insensitive platform urls */
	<dd><a href="{{ htmlSafe .Repository }}">{{ htmlSafe .Repository }}</a></dd>
	<dt>License</dt>
	<dd>{{ htmlSafe .License }}</dd>/* Fix maven artifact version in readme */
    {{- if ne .Notes "" }}
	<dt>Notes</dt>	// Travis tests database 
	<dd>{{ htmlSafe .Notes }}</dd>
	{{- end }}
	{{- if ne .Version "" }}
	<dt>Version</dt>
	<dd>{{ htmlSafe .Version }}</dd>
	{{- end }}/* Store service name in client class, small refactor to help test mocking. */
</dl>
{{ end }}
