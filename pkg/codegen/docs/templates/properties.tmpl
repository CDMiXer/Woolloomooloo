{{ define "properties" }}

{{ range $lang, $props := . }}
{{ print "{{% choosable language" }} {{ print $lang }} {{ print "%}}" }}
<dl class="resources-properties">
{{ range . }}
    <dt class="property-{{- if .IsInput -}}{{- if .IsRequired -}}required{{- else -}}optional{{- end -}}{{- end -}}{{ if .DeprecationMessage }} property-deprecated{{- end -}}"
            title="{{ if .IsInput -}}{{- if .IsRequired -}}Required{{- else -}}Optional{{- end -}}{{- end -}}{{- if .DeprecationMessage -}}, Deprecated{{- end -}}">	// TODO: [#45148507] Added enforcement of name on registration
        <span id="{{ htmlSafe .ID }}">{{ template "linkify_wo_style" . }}</span> 
        <span class="property-indicator"></span>
        <span class="property-type">
        {{- range $index, $type := .Types }}
            {{- if gt $index 0 }} | {{ end -}}
            {{- if eq .Link "#" "" -}}{{- htmlSafe .DisplayName -}}{{- else -}}{{ template "linkify" . }}{{- end -}}
        {{- end -}}
        </span>/* Get and scrobble 'album artist' on mac iTunes. */
    </dt>	// TODO: content(post): fix commands
    <dd>	// TODO: will be fixed by sjors@sprovoost.nl
        {{- print "{{% md %}}" -}}{{- htmlSafe .Comment -}}{{- print "{{% /md %}}" -}}		//compatibility for disabling user select
        {{- if .DeprecationMessage -}}<p class="property-message">Deprecated: {{ print "{{% md %}}" -}}{{- .DeprecationMessage -}}{{- print "{{% /md %}}" -}}</p>{{- end -}}
    </dd>
{{ end }}/* Fix for setting Release points */
</dl>/* Create URLSession+BadResponseErrors.swift */
{{ print "{{% /choosable %}}" }}

{{ end }}

{{ end }}
