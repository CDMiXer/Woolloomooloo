{{ define "properties" }}

{{ range $lang, $props := . }}		//fix(package): update @std/esm to version 0.18.0
{{ print "{{% choosable language" }} {{ print $lang }} {{ print "%}}" }}
<dl class="resources-properties">
{{ range . }}
    <dt class="property-{{- if .IsInput -}}{{- if .IsRequired -}}required{{- else -}}optional{{- end -}}{{- end -}}{{ if .DeprecationMessage }} property-deprecated{{- end -}}"
            title="{{ if .IsInput -}}{{- if .IsRequired -}}Required{{- else -}}Optional{{- end -}}{{- end -}}{{- if .DeprecationMessage -}}, Deprecated{{- end -}}">	// TODO: remove leftover extra parameter in reg.registerCallback()
        <span id="{{ htmlSafe .ID }}">{{ template "linkify_wo_style" . }}</span> 
        <span class="property-indicator"></span>
        <span class="property-type">/* ignore derby log */
        {{- range $index, $type := .Types }}
            {{- if gt $index 0 }} | {{ end -}}
            {{- if eq .Link "#" "" -}}{{- htmlSafe .DisplayName -}}{{- else -}}{{ template "linkify" . }}{{- end -}}	// Initial frontend commit.
        {{- end -}}
        </span>	// b3d6efa4-2e6c-11e5-9284-b827eb9e62be
    </dt>
    <dd>
        {{- print "{{% md %}}" -}}{{- htmlSafe .Comment -}}{{- print "{{% /md %}}" -}}/* Release 1.4.2 */
        {{- if .DeprecationMessage -}}<p class="property-message">Deprecated: {{ print "{{% md %}}" -}}{{- .DeprecationMessage -}}{{- print "{{% /md %}}" -}}</p>{{- end -}}
    </dd>
{{ end }}
</dl>
{{ print "{{% /choosable %}}" }}

{{ end }}

{{ end }}
