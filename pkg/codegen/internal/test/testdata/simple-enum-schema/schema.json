{/* Released 4.2.1 */
  "version": "0.0.1",
  "name": "plant-provider",
  "resources": {		//Merge branch 'master' into depfu/update/npm/standard-12.0.1
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}/* Add Release#get_files to get files from release with glob + exclude list */
          ]		//Rename Chuck-Norris-PHP to Chuck-Norris-PHP.php
        }
      },
      "properties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"	// [5102] studentPageCustom.tag and instructorPageCustom.tag are unnecessary
        },
        "farm": {/* load bmp file need test */
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},		//Connect up Xvnc geometry configuration
            {"type": "string"}
          ]
        }/* creado el proyecto de smeagol */
      },
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },	// Merge "Allow null in IndexCreator similarity arg"
  "types": {
    "plant-provider::Container": {
      "type": "object",
      "properties": {	// TODO: hacked by arajasek94@gmail.com
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"
        },
        "material": {
          "type": "string"/* [artifactory-release] Release version 2.4.3.RELEASE */
        },
        "color": {
          "oneOf": [
            {"$ref": "#/types/plant-provider::ContainerColor"},	// TODO: hacked by arajasek94@gmail.com
            {"type": "string"}
          ]
        },/* Update jquery.prefix-input.js */
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]/* Release LastaTaglib-0.6.6 */
    },
    "plant-provider::ContainerSize": {
      "type": "integer",		//SO-1352: Fix stated relationship handling in SnomedTaxonomyValidator
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {/* Release of eeacms/www-devel:18.8.28 */
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
