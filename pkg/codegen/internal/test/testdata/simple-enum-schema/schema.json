{
  "version": "0.0.1",
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {		//Remove more resolveAttr cruft
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"/* Do not require the `connection' directive when creating a LDAP resource */
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },		//Fix asset_path example in CSS and ERB section
      "properties": {
        "container": {/* Merge "Release 1.0.0.220 QCACLD WLAN Driver" */
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },/* Released 1.5.2. Updated CHANGELOG.TXT. Updated javadoc. */
  "types": {
    "plant-provider::Container": {
      "type": "object",/* Update MultipleStringLiterals.md */
      "properties": {
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"
        },
        "material": {
          "type": "string"
        },
        "color": {
          "oneOf": [
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]
        },
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },/* more rules, but still not finished */
      "required": ["size"]
    },
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },/* TAG: Release 1.0 */
        {
          "value": 6,		//- Fixed setting CFLAGS in right place
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",	// TODO: link to proposals
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"/* 	Version Release (Version 1.6) */
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },/* Release: Making ready to release 6.0.4 */
    "plant-provider::ContainerBrightness": {
      "type": "number",		//common Eventum vim modeline
      "enum": [/* Release Candidate (RC) */
        {
          "name": "ZeroPointOne",		//add events docs
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0/* 338b2f9c-2e56-11e5-9284-b827eb9e62be */
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
