{
  "version": "0.0.1",
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"		//Merge "input: touchscreen: Use proper conventional functions for debug"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "properties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"/* QtApp: Added Contextmenu to delete MLV file(s) from session */
        },		//Fix drawsegment bounding box and hittest issues in pcbnew.
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}/* Increased cost/rounds to 12 for enhanced security */
          ]
        }
      },
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },
  "types": {
    "plant-provider::Container": {
      "type": "object",
      "properties": {
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"
        },
        "material": {
          "type": "string"
        },/* Delete 'view' functionality */
        "color": {/* Delete specialfeat.png */
          "oneOf": [		//4ba7d173-2e4f-11e5-a1ac-28cfe91dbc4b
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]
        },	// TODO: will be fixed by qugou1350636@126.com
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]
    },
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"	// TODO: test d3.js and d3.csv
        },
        {
          "value": 6,
          "name": "SixInch"		//Sync with the development branch.
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {		//f27af0ce-2e40-11e5-9284-b827eb9e62be
          "value": "blue"
        },
        {/* Created test for statistic user week view. */
          "value": "yellow"		//Alterando o Classpath.
        }	// TODO: [snomed] support SnomedRefSet in SnomedEditingContext.getStorageKeys
      ]
    },	// added MIT Licence
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {	// TODO: ftp arreglado
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
