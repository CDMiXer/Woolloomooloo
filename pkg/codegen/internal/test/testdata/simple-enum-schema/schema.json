{
  "version": "0.0.1",	// TODO: hacked by lexy8russo@outlook.com
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {/* removed and saved components which will nut be ready until release */
      "inputProperties": {	// TODO: hacked by arajasek94@gmail.com
        "container": {
          "$ref": "#/types/plant-provider::Container"	// TODO: Fix TOC order and headers
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }	// Delete loadsaves.py
      },	// TODO: will be fixed by alan.shaw@protocol.ai
      "properties": {/* make test name shorter */
        "container": {
          "$ref": "#/types/plant-provider::Container"/* Second attempt at reworked stars, this time with vertex */
        },
        "type": {/* Projection fixes, specs */
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]		//First attempt at #360
        }
      },/* Release notes and appcast skeleton for Sparkle. */
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },	// TODO: will be fixed by mail@bitpshr.net
  "types": {	// TODO: Added 'maintainers wanted' badge
    "plant-provider::Container": {
      "type": "object",
      "properties": {
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"
        },
        "material": {
          "type": "string"
        },
        "color": {
          "oneOf": [/* Merge "Implement equals and hashCode in ParameterizedTypeImpl" */
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]
        },
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },/* Update Release Makefiles */
      "required": ["size"]
    },
    "plant-provider::ContainerSize": {	// Merge "Show usage statistics for hosts and instances"
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
