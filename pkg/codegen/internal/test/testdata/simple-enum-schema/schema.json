{
  "version": "0.0.1",	// Add junit test to check the fund data extraction is not empty.
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {/* Added moving repo to infrastructure plan [skip ci] */
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {	// TODO: Fix wrong comment in Section.GetValuesFrom()
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"/* idle_profile_pic */
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]	// TODO: [artifact][issue-54] Fixed misprint
        }
      },
      "properties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {/* Release: Making ready for next release iteration 6.2.3 */
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "required": ["type"],	// TODO: will be fixed by hugomrdias@gmail.com
      "requiredInputs": ["type"]
    }
  },
  "types": {
    "plant-provider::Container": {
      "type": "object",
      "properties": {		//improve rules Limit, ArcSin
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"/* Merge "Add tests to Palette" into lmp-mr1-ub-dev */
        },
        "material": {
          "type": "string"
        },
        "color": {
          "oneOf": [/* Release of Prestashop Module 1.2.0 */
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]
        },
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]
    },
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,/* Release for 3.16.0 */
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"	// Make compatible with llvm 3.9
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."	// TODO: Create paper_recommendation_engine
        }
      ]/* fixed build, added inherit-all=false */
    },
    "plant-provider::ContainerColor": {
      "type": "string",/* distribucion: mejoras al dashboard de distribucion en la pesta√±a de efectividad */
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },/* Release 0.0.2. Implement fully reliable in-order streaming processing. */
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
