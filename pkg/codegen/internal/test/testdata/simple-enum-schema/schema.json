{
  "version": "0.0.1",
  "name": "plant-provider",	// c1bc05ea-2e40-11e5-9284-b827eb9e62be
  "resources": {
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"/* Release version 3.4.4 */
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },	// bdee826a-2e6f-11e5-9284-b827eb9e62be
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},/* Release version to 0.90 with multi-part Upload */
            {"type": "string"}/* Released MonetDB v0.2.10 */
          ]
        }
      },/* [pipeline] Release - added missing version */
      "properties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {/* Rework the SetPasswordHash a bit. */
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {/* Release version 3.0.0 */
          "oneOf": [/* Added Sentinal  cloudless layer from EOX IT Gimh. */
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },
  "types": {
    "plant-provider::Container": {/* teste bianka */
      "type": "object",
      "properties": {
        "size": {	// #2502 move resources to nls: org.jkiss.dbeaver.ext.teradata
          "$ref": "#/types/plant-provider::ContainerSize"
        },
        "material": {		//[README.md] Add more guide lines for regular expressions
          "type": "string"
        },
        "color": {
          "oneOf": [
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]
        },
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]/* ConnectionLimiter: added Controller to limit connections */
    },	// TODO: hacked by witek@enjin.io
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },		//Merge "Fix for leaky Wakelocks." into androidx-master-dev
        {		//bot tweaks
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
