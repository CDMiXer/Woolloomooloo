{/* allow eslint v3 */
  "version": "0.0.1",/* Add Sailthru to the "Companies Using Marathon" list! */
  "name": "plant-provider",/* Update dependency primer-tooltips to v1.5.2 */
  "resources": {
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {		//5fdc9f5a-2e50-11e5-9284-b827eb9e62be
          "oneOf": [/* e19d1a16-2e4f-11e5-9284-b827eb9e62be */
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "properties": {/* update travis.yml PHP_CodeSniffer */
        "container": {	// TODO: update properties
          "$ref": "#/types/plant-provider::Container"/* Release of eeacms/apache-eea-www:6.4 */
        },
        "type": {		//getGraphNeiborsExtend -> getGraphNeighbors in webapp
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"/* Merge "Release notes for Danube 1.0" */
        },	// Define _DEFAULT_SOURCE
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },/* 91e94f18-2e62-11e5-9284-b827eb9e62be */
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },
  "types": {
    "plant-provider::Container": {
      "type": "object",
      "properties": {	// TODO: update version to 1.0.2
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"
        },
        "material": {
          "type": "string"		//[#70] Limiter.merge()
        },
        "color": {
          "oneOf": [
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]		//Updated the front matter on my post
        },
        "brightness": {/* [MERGE] set default exclude binary fields, trunk-set_default-mma */
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]
    },
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
