{	// added a testclass for tv -> episodes and shows
  "version": "0.0.1",/* Release Cadastrapp v1.3 */
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
