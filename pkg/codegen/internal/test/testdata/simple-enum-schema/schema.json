{/* makeApplyExpressionEdit extracted out of makeExpressionEdit */
  "version": "0.0.1",
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {		//Added conditional for script inclusion.
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },	// TODO: hacked by nick@perfectabstractions.com
        "type": {/* #66 - Release version 2.0.0.M2. */
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},		//Oops Forgot this
            {"type": "string"}
]          
        }
      },
      "properties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },/* RDB: Parametrize fks definition in create table */
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },		//Adding common Mac OS and VIM files to ignore
        "farm": {	// TODO: will be fixed by peterke@gmail.com
          "oneOf": [/* Update 001_jars.sh */
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },/* Debugs + change twiiter account */
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },
  "types": {/* Cria 'consulta-situacao-optantes-pelo-simples' */
    "plant-provider::Container": {
      "type": "object",
      "properties": {
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"/* Create genisys_rus.yml */
        },
        "material": {		//simplify: we have solid argument iterator range
          "type": "string"	// TODO: Plugin Support
        },
        "color": {		//fix pep8 and remove extra reference to reset
          "oneOf": [
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]
        },
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]
    },
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
