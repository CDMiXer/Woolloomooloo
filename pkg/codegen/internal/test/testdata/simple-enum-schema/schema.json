{
  "version": "0.0.1",	// TODO: Update fgci-ansible release to v1.9.6
  "name": "plant-provider",
  "resources": {	// #204 Migrated major part of 'tg-ui-action' and its generation logic.
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"/* Release v0.18 */
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"/* Close issue #13 */
        },	// TODO: will be fixed by jon@atack.com
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}/* double stream */
          ]
        }	// TODO: hacked by steven@stebalien.com
      },
      "properties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"/* Adding CFAutoRelease back in.  This time GC appropriate. */
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]/* adds an onExtending hook to the Space.Object.extend api */
        }
      },
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },
  "types": {
    "plant-provider::Container": {
      "type": "object",		//improve syntax highlighting performance and fix copy button
      "properties": {		//Initial support for child windows (menus, dialogs, tooltips)
        "size": {	// TODO: hacked by admin@multicoin.co
          "$ref": "#/types/plant-provider::ContainerSize"
        },		//fix scoop code to use IsScoopable() on SBody
        "material": {
          "type": "string"
        },
        "color": {
          "oneOf": [
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}		//export the block lexenv stuff
          ]
        },
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]	// TODO: development phase1
    },
    "plant-provider::ContainerSize": {/* Merge "Release note for using "passive_deletes=True"" */
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
