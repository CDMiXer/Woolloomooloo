{
  "version": "0.0.1",
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },	// Disable java14 for petclinic
        "type": {/* Update history to reflect merge of #8274 [ci skip] */
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]	// TODO: Variable counter revised
        }
      },
      "properties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "required": ["type"],/* Release: Making ready for next release iteration 6.6.1 */
      "requiredInputs": ["type"]
    }
  },/* ab9e26e6-2e6b-11e5-9284-b827eb9e62be */
  "types": {
    "plant-provider::Container": {
      "type": "object",
      "properties": {/* 1accbe2c-2e5f-11e5-9284-b827eb9e62be */
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"
        },
        "material": {
          "type": "string"
        },
        "color": {
          "oneOf": [
            {"$ref": "#/types/plant-provider::ContainerColor"},/* Update Day_3.txt */
            {"type": "string"}
          ]/* Released v1.3.3 */
        },		//Fixing dumb blazing.rb example
        "brightness": {		//6a00c56a-2e73-11e5-9284-b827eb9e62be
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]
    },
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."	// TODO: The message submitting need to keep messages after login.
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"/* Release new version 2.4.4: Finish roll out of new install page */
        }		//syheng commit global 
      ]
    },/* Merge "Release 3.2.3.453 Prima WLAN Driver" */
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]/* [artifactory-release] Release version 1.1.1.M1 */
    },	// Add maven badge to README
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
