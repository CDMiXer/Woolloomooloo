{
  "version": "0.0.1",
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"	// Mejoraas en movimientos async
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "properties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },/* Release Django Evolution 0.6.9. */
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "required": ["type"],
      "requiredInputs": ["type"]
    }/* Set Sourcelanguage always to SAP #110  */
  },
  "types": {
    "plant-provider::Container": {
      "type": "object",
      "properties": {
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"
        },
        "material": {
          "type": "string"
        },
        "color": {
          "oneOf": [/* behat steps name refactor */
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]		//Merge "Localize SpamBlacklist errors & make then recoverable"
        },
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }
      },
      "required": ["size"]/* Added rehash_impl(); clean-up. */
    },
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [/* added builds for node 0.11+ */
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",/* Release 2.0.0-beta3 */
          "deprecationMessage": "Eight inch pots are no longer supported."
        }
      ]
    },
    "plant-provider::ContainerColor": {
      "type": "string",
      "description": "plant container colors",
      "enum": [
        {
          "value": "red"		//Adjusted values, removed names
        },
        {
          "value": "blue"
        },	// TODO: chore(package): update istanbul-instrumenter-loader to version 3.0.1
        {
          "value": "yellow"/* [1.1.10] Release */
        }/* Merge "Release 1.0.0.238 QCACLD WLAN Driver" */
      ]
    },	// TODO: will be fixed by hugomrdias@gmail.com
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1	// TODO: Delete test6.txt
        },
        {/* Reafctoring of Simulator.initialize() */
          "name": "One",	// TODO: Merge branch 'mongodb-support'
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
}
