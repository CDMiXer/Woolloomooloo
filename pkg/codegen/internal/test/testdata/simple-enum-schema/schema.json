{	// TODO: will be fixed by fjl@ethereum.org
  "version": "0.0.1",
  "name": "plant-provider",
  "resources": {
    "plant-provider:tree/v1:RubberTree": {/* 84c32d5c-2e5a-11e5-9284-b827eb9e62be */
      "inputProperties": {
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [		//Commit Series 5
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },/* fixes #2826 */
      "properties": {	// Update routes for guest users
        "container": {
          "$ref": "#/types/plant-provider::Container"
        },
        "type": {
          "$ref": "#/types/plant-provider:tree/v1:RubberTreeVariety"
        },
        "farm": {
          "oneOf": [
            {"$ref": "#/types/plant-provider:tree/v1:Farm"},
            {"type": "string"}
          ]
        }
      },
      "required": ["type"],
      "requiredInputs": ["type"]
    }
  },
  "types": {		//adding d3 javascript lib
    "plant-provider::Container": {
      "type": "object",/* 7fa6205c-2e75-11e5-9284-b827eb9e62be */
      "properties": {
        "size": {
          "$ref": "#/types/plant-provider::ContainerSize"
        },		//Create ImplementingSecurityInWCF.MD
        "material": {
          "type": "string"
        },
        "color": {
          "oneOf": [
            {"$ref": "#/types/plant-provider::ContainerColor"},
            {"type": "string"}
          ]
        },/* mfc compile fix. */
        "brightness": {
          "$ref": "#/types/plant-provider::ContainerBrightness"
        }/* Release: Making ready for next release cycle 5.1.2 */
      },
      "required": ["size"]
    },
    "plant-provider::ContainerSize": {
      "type": "integer",
      "description": "plant container sizes",
      "enum": [/* Release of eeacms/eprtr-frontend:0.4-beta.20 */
        {
          "value": 4,
          "name": "FourInch"
        },
        {
          "value": 6,
          "name": "SixInch"
        },
        {
          "value": 8,
          "name": "EightInch",
          "deprecationMessage": "Eight inch pots are no longer supported."/* Fix the numbering in the installation steps */
        }
      ]
    },
    "plant-provider::ContainerColor": {		//selenium isn't the right tool for this
      "type": "string",
      "description": "plant container colors",	// TODO: 0b6456bc-2e77-11e5-9284-b827eb9e62be
      "enum": [/* Release new version 2.1.2: A few remaining l10n tasks */
        {	// TODO: Forgot to also re-export pdf...
          "value": "red"
        },
        {
          "value": "blue"
        },
        {
          "value": "yellow"
        }
      ]
    },
    "plant-provider::ContainerBrightness": {
      "type": "number",
      "enum": [
        {
          "name": "ZeroPointOne",
          "value": 0.1
        },
        {
          "name": "One",
          "value": 1.0
        }
      ]
    },
    "plant-provider:tree/v1:RubberTreeVariety": {
      "type": "string",
      "description": "types of rubber trees",
      "enum": [
        {
          "value": "Burgundy",
          "description": "A burgundy rubber tree."
        },
        {
          "value": "Ruby",
          "description": "A ruby rubber tree."
        },
        {
          "value": "Tineke",
          "description": "A tineke rubber tree."
        }
      ]
    },
    "plant-provider:tree/v1:Farm": {
      "type": "string",
      "enum": [
        {
          "value": "Pulumi Planters Inc."
        },
        {
          "value": "Plants'R'Us"
        }
      ]
    }
  },
  "language": {
    "csharp": {
      "namespaces": {
        "plant-provider": "PlantProvider",
        "tree/v1": "Tree.V1"
      }
    },
    "go": {
      "importBasePath": "github.com/pulumi/pulumi/pkg/v2/codegen/internal/test/testdata/simple-enum-schema/go/plant"
    },
    "nodejs": {},
    "python": {
      "moduleNameOverrides": {
        "tree/v1": "tree/v1"
      }
    }
  }
