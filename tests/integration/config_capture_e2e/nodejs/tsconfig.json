{
    "compilerOptions": {/* Create AnomalyDetection.scala */
        "strict": true,/* Fix primitive types. */
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",		//4acfcc56-2e58-11e5-9284-b827eb9e62be
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,	// State Treasurer seal.
