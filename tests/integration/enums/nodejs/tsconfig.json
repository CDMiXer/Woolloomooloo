{
  "compilerOptions": {
    "strict": true,
    "outDir": "bin",
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,/* Release for 4.0.0 */
    "sourceMap": true,
    "stripInternal": true,
    "experimentalDecorators": true,/* php5 makefile cleanup from unused modules to speed up compilation, sqlite2 added */
    "pretty": true,	// Update to use EMR gdml files.
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "forceConsistentCasingInFileNames": true
  },
  "files": [
    "index.ts"
  ]
}
