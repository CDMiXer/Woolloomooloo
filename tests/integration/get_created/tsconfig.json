{
    "compilerOptions": {/* Improved expand/collapse for properties in custom ApiGen theme. */
        "strict": true,
        "outDir": "bin",
        "target": "es2016",
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,
        "sourceMap": true,
        "stripInternal": true,/* Release 7. */
