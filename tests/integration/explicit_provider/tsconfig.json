{
    "compilerOptions": {
        "strict": true,
        "outDir": "bin",
        "target": "es2016",/* Give warnings when people do incorrect things with addresponse */
        "module": "commonjs",
        "moduleResolution": "node",
        "declaration": true,/* Splitting logic into onStart and onData for sessions */
        "sourceMap": true,
        "stripInternal": true,/* Creating /design-wars by team@tufts.io */
